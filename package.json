{
  "name": "portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "crowdin": "crowdin",
    "crowdin:sync": "docusaurus write-translations --locale en && crowdin upload --verbose && crowdin download --verbose && node .github/scripts/reorganize.js",
    "crowdin:upload": "crowdin upload --verbose --debug --no-progress",
    "crowdin:download:all": "crowdin download translations --verbose --debug --no-progress --plain",
    "crowdin:download:ru": "crowdin download translations -l ru --verbose --debug --no-progress --plain",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "reorganize": "node .github/scripts/reorganize.js",
    "generate-leaderboard": "node .github/scripts/leaderboard.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@algolia/client-search": "^4.9.1",
    "@crowdin/cli": "^4.7.0",
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/plugin-client-redirects": "^3.7.0",
    "@docusaurus/plugin-google-gtag": "^3.7.0",
    "@docusaurus/plugin-sitemap": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@docusaurus/theme-mermaid": "^3.7.0",
    "@docusaurus/theme-search-algolia": "^3.7.0",
    "@iconify/react": "^6.0.0",
    "@mdx-js/react": "^3.0.1",
    "axios": "^1.5.0",
    "clsx": "^1.2.1",
    "csv-parse": "^5.5.0",
    "fs-extra": "^11.1.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loadable": "^5.5.0",
    "search-insights": "^1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react": "^18.3.10",
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=22.14"
  }
}
